  "query": "\n        SELECT id, client_id, name, registrar, nameservers, registration_date,\n               expiry_date, auto_renew, dns_records, notes, created_at, updated_at\n        FROM domains\n        WHERE expiry_date <= CURRENT_DATE + ($1 || ' days')::INTERVAL\n        ORDER BY expiry_date ASC\n        ",
